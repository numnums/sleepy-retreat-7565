              <div><span class="supporter-text">Fox Family Foundation</span></div>
              <div><span class="supporter-text">Witkin Foundation</span></div>
            </div>
            <div class="supporter">
              <%= image_tag "supporters/first_5_alameda.jpg" %>
            </div>
            <div class="supporter">
              <%= image_tag "supporters/wpadefbd0f_06.png" %>
            </div>
            <div class="supporter">
              <%= image_tag "supporters/pge.jpg" %>
            </div>
          </div>
          <div class="col-md-4">
            <h3>In-kind donations</h3>
            <div class="supporter">
              <%= image_tag "supporters/peeps.jpg" %>
            </div>
            <div class="supporter">
              <%= image_tag "supporters/needlogo.jpg" %>
